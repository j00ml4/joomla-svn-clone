<?xml version="1.0" encoding="utf-8"?>
	<!-- $Id$ -->
<form>
	<fieldset>
		<field
			name="id"
			label="JGLOBAL_RECORD_NUMBER"			
			type="hidden"
			default="0"
			required="true"
			readonly="true" />

		<field
			name="template"
			type="text"
			label="COM_TEMPLATES_FIELD_TEMPLATE_LABEL"
			description="COM_TEMPLATES_FIELD_TEMPLATE_DESC"
			class="readonly"
			size="30"
			readonly="true"
			filter="unset" />

		<field
			name="client_id"
			type="client"
			label="COM_TEMPLATES_FIELD_CLIENT_LABEL"
			description="COM_TEMPLATES_FIELD_CLIENT_DESC"
			class="readonly"
			default="0"
			readonly="true" />

		<field
			name="home"
			type="list"
			label="COM_TEMPLATES_FIELD_HOME_LABEL"
			description="COM_TEMPLATES_FIELD_HOME_DESC"
			class="inputbox"
			default="0"
			readonly="true">
			<option
				value="1">
				JYes</option>
			<option
				value="0">
				JNo</option>
		</field>

		<field
			name="title"
			type="text"
			class="inputbox"
			default=""
			size="50"
			label="COM_TEMPLATES_FIELD_TITLE_LABEL"
			description="COM_TEMPLATES_FIELD_TITLE_DESC"
			required="true" />

		<field
			name="params"
			type="hidden" />
	</fieldset>
</form>