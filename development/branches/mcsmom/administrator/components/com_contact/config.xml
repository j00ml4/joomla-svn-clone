			description="COM_CONTACT_FIELD_PARAMS_STREET_ADDRESS_DESC"
		>
			<option value="0">JHIDE</option>
			<option value="1">JSHOW</option>
		</field>

		<field name="show_suburb" type="radio" default="1"
