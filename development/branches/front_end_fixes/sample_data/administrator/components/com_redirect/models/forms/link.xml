<?xml version="1.0" encoding="utf-8"?>
	<!-- $Id$ -->
<form>
	<fieldset>
		<field
			id="id"
			name="id"
			type="hidden"
			default="0"
			required="true"
			readonly="true" />

		<field
			name="old_url"
			type="text"
			class="inputbox"
			label="COM_REDIRECT_FIELD_OLD_URL_LABEL"
			description="COM_REDIRECT_FIELD_OLD_URL_DESC"
			size="50"
			default=""
			required="true" />

		<field
			name="new_url"
			type="text"
			class="inputbox"
			label="COM_REDIRECT_FIELD_NEW_URL_LABEL"
			description="COM_REDIRECT_FIELD_NEW_URL_DESC"
			size="50"
			default="" />

		<field
			name="comment"
			type="text"
			class="inputbox"
			label="COM_REDIRECT_FIELD_COMMENT_LABEL"
			description="COM_REDIRECT_FIELD_COMMENT_DESC"
			size="40"
			default="" />

		<field
			name="published"
			type="list"
			label="JFIELD_PUBLISHED_LABEL"
			description="JFIELD_PUBLISHED_DESC"
			class="inputbox"
			size="1"
			required="true"
			default="1">
			<option
				value="1">
				JOPTION_ENABLED</option>
			<option
				value="0">
				JOPTION_DISABLED</option>
			<option
				value="2">
				JOPTION_ARCHIVED</option>
			<option
				value="-2">
				JOPTION_TRASHED</option>
		</field>

		<field
			name="referer"
			type="text"
			id="referer"
			class="inputbox"
			label="COM_REDIRECT_FIELD_REFERRER_LABEL"
			size="50"
			default=""
			readonly="true" />

		<field
			name="created_date"
			type="text"
			id="created_date"
			class="inputbox"
			label="COM_REDIRECT_FIELD_CREATED_DATE_LABEL"
			size="20"
			default=""
			readonly="true" />

		<field
			name="updated_date"
			type="text"
			id="updated_date"
			class="inputbox"
			label="COM_REDIRECT_FIELD_UPDATED_DATE_LABEL"
			size="20"
			default=""
			readonly="true" />
	</fieldset>
</form>