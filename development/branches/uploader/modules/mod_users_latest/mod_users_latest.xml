<?xml version="1.0" encoding="utf-8"?>
<extension
	type="module"
	version="1.6.0"
	client="site"
	method="upgrade">
	<name>Latest Users</name>
	<author>Joomla! Project</author>
	<creationDate>December 2009</creationDate>
	<copyright>Copyright (C) 2005 - 2010 Open Source Matters. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<authorEmail>admin@joomla.org</authorEmail>
	<authorUrl>www.joomla.org</authorUrl>
	<version>1.6.0</version>
	<description>DESCUSERSLATEST</description>
	<files>
		<filename
			module="mod_users_latest">mod_users_latest.php</filename>
		<folder>tmpl</folder>
		<filename>helper.php</filename>
		<filename>index.php</filename>
		<filename>mod_users_latest.xml</filename>
	</files>
	<fields>
		<field
			name="shownumber"
			type="text"
			default="5"
			label="Users_Latest_number_Label"
			description="Unsers_Latest_number_Desc">
		</field>
		<field
			name="linknames"
			type="list"
			default="0"
			label="Users_Latest_Link_label"
			description="Users_Latest_Link_desc">
			<option
				value="0">JNo</option>
			<option
				value="1">JYes</option>
		</field>
		<field
			name="linktowhat"
			type="list"
			default="0"
			label="Users_Latest_Linktowhat_label"
			description="Users_Latest_Linktowhat_desc">
			<option
				value="0">Profile</option>
			<option
				value="1">Contact</option>
		</field>
	</fields>
	<fields
		group="advanced">
		<field
			name="layout"
			type="modulelayout"
			label="JFIELD_ALT_LAYOUT_LABEL"
			description="JFIELD_ALT_MODULE_LAYOUT_DESC" />

		<field
			name="moduleclass_sfx"
			type="text"
			default=""
			label="MODULES_FIELD_MODULECLASS_SFX_LABEL"
			description="MODULES_FIELD_MODULECLASS_SFX_DESC" />

		<field
			name="cache"
			type="list"
			default="0"
			label="MODULES_FIELD_CACHING_LABEL"
			description="MODULES_FIELD_CACHING_DESC">
			<option
				value="0">MODULES_FIELD_VALUE_NOCACHING</option>
		</field>
	</fields>
</extension>
