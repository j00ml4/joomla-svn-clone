<?xml version="1.0" encoding="utf-8"?>
	<!-- $Id$ -->
<form>
	<fields>
		<field name="id" type="hidden"
			default="0"
			readonly="true"
			required="true"
		/>

		<field name="template" type="text"
			class="readonly"
			description="Templates_Field_Template_Desc"
			filter="unset"
			label="Templates_Field_Template_Label"
			readonly="true"
			size="30"
		/>

		<field name="client_id" type="client"
			class="readonly"
			default="0"
			description="Templates_Field_Client_Desc"
			label="Templates_Field_Client_Label"
			readonly="true"
		/>

		<field name="home" type="list"
			class="inputbox"
			default="0"
			description="Templates_Field_Home_Desc"
			label="Templates_Field_Home_Label"
			readonly="true"
		>
			<option value="1">JYes</option>
			<option value="0">JNo</option>
		</field>

		<field name="title" type="text"
			class="inputbox"
			default=""
			description="Templates_Field_Title_Desc"
			label="Templates_Field_Title_Label"
			required="true"
			size="50"
		/>

		<field name="params" type="hidden" />
	</fields>
</form>