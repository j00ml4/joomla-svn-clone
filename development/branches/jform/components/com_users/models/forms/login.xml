<?xml version="1.0" encoding="utf-8"?>
	<!-- $Id$ -->
<form>
	<fieldset label="Users_Login_Default_Label">

		<field name="username" type="text"
			class="validate-username"
			field="id"
			filter="username"
			label="Users_Login_Username_Label"
			required="true"
			size="25"
			validate="username"
		/>

		<field name="password" type="password"
			class="validate-password"
			filter="raw"
			label="Users_Login_Password_Label"
			required="required"
			size="25"
		/>
	</fieldset>
</form>