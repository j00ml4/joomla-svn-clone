<?xml version="1.0" encoding="utf-8"?>
<install
	type="module"
	version="1.0.0">
	<name>Latest Users</name>
	<author>Joomla! Project</author>
	<creationDate>December 2009</creationDate>
	<copyright>Copyright (C) 2005 - 2009 Open Source Matters. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<authorEmail>admin@joomla.org</authorEmail>
	<authorUrl>www.joomla.org</authorUrl>
	<version>1.0.0</version>
	<description>DESCUSERSLATEST</description>
	<files>
		<filename
			module="mod_users_latest">mod_users_latest.php</filename>
	</files>
	<fields>
		<field
			name="shownumber"
			type="text"
			default="5"
			label="Users_Latest_number_Label"
			description="Unsers_Latest_number_Desc">
			</field>	
		<field
			name="linknames"
			type="list"
			default="0"
			label="Users_Latest_Link_label"
			description="Users_Latest_Link_desc"	>
			<option
				value="0">JNo</option>
			<option
				value="1">JYes</option>	
		</field>
		<field
			name="linktowhat"
			type="list"
			default="0"
			label="Users_Latest_Linktowhat_label"
			description="Users_Latest_Linktowhat_desc"	>
			<option
				value="0">Profile</option>
			<option
				value="1">Contact</option>	
		</field>
	</fields>
	<fields
		group="advanced">
		<field
			name="layout"
			type="modulelayout"
			label="JField_Alt_Layout_Label"
			description="JField_Alt_Module_Layout_Desc" />

		<field
			name="moduleclass_sfx"
			type="text"
			default=""
			label="Module_Field_Moduleclass_sfx_Label"
			description="Module_Field_Moduleclass_sfx_Desc" />

		<field
			name="cache"
			type="list"
			default="0"
			label="Module_Field_Caching_Label"
			description="Module_Field_Caching_Desc">
			<option
				value="0">No caching</option>
		</field>
	</fields>
</install>
